package com.rays.javabasic;

public class splender {
	public static void main(String[] args) {
		int gear=4;
		
		switch (gear){
		case(1):
			System.out.println("the speed of bike is 20km/hr");
		break;
		
		case(2):
			System.out.println("the speed of bike is 40km/hr");
		break;
		
		case(3):
			System.out.println("the speed of bike is 60km/hr");
		break;
		
		case(4):
			System.out.println("the speed of bike is 80km/hr");
		break;
		
			
		}
		
		
		
	}
	
	

}
